{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es5",
    "jsx": "react-jsx",
    "module": "es6",
    "declaration": true,
    "outDir": "./distt",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "."
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "typings/*.d.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
